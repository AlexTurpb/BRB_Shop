<h2>Registered users</h2>
<div class="container">	
	<table class="table table-striped">
		<thead>
			<tr>
				<th>No.</th>
				<th>Name</th>
				<th>Phone number</th>
				<th>Registered on</th>
				<th>Barber</th>
				<th>Color</th>
			</tr>
		</thead>
		<tbody>
			<tr><%@show_loosers.each do |row|%>
				<td><%="#{row['id']}"%></td>
				<td><%="#{row['username']}"%></td>
				<td><%="#{row['phone']}"%></td>
				<td><%="#{row['datestamp']}"%></td>
				<td><%="#{row['barber']}"%></td>
				<td><%="#{row['color']}"%></td>
			</tr><%end%>
		</tbody>	
	</table>
</div>